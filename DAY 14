208A. Dubstep

#include <bits/stdc++.h>
using namespace std;
 
int main(){
    string str;
    int i;
    cin>>str;
    for(i = 0; i<str.length(); i++)
    {
        if(str[i] == 'W' && str[i+1] == 'U' && str[i+2] == 'B'){
            i = i +2;
            cout<<" ";
        }
        else{
            cout<<str[i];
        }
    }
}


136A. Presents

#include <iostream>
#include <vector>
 
using namespace std;
 
int main()
{
    int n, p;
    cin >> n;
    vector<int> f(n+1);
    for (int i = 1; i <= n; ++i)
    {
        cin >> p;
        f[p] = i;
    }
    cout << f[1];
    for (int i = 2; i <= n; ++i)
    {
        cout << " " << f[i];
    }
    cout << endl;
    return 0;
}
