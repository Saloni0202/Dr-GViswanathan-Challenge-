200B. Drinks

#include <iostream>
using namespace std;
 
int main(){
    int n;
    cin>>n;
    long double res = 0;
    for(int i = 0; i<n; i++){
        long double p;
        cin>>p;
        res+=p/100;
    }
    cout<<res*100/n;
    return 0;
}


96A. Football

#include <iostream>
#include <string>
using namespace std;
 
int main(){
    string s;
    cin>>s;
    
    int contiguous = 1;
    for(size_t i = 1; i<s.length(); ++i){
        if(s[i]==s[i-1]){
            contiguous += 1;
            if(contiguous == 7){
                cout<<"YES"<<endl;
                return 0;
            }
        }
        else{
            contiguous = 1;
        }
    }
    
    cout<<"NO"<<endl;
    return 0;
}
